{
	"extends": "@parcel/config-default",
	"optimizers": {
		"*.css": [
			"@parcel/optimizer-cssnano"
		],
		"*.html": [
			"@parcel/optimizer-htmlnano"
		],
		"*.{png,jpg,jpeg,svg,...}": [
			"@parcel/optimizer-imagemin"
		]
	},
	"transformers": {
		"*.{js,mjs,jsx,cjs,ts,tsx}": [
			"@parcel/transformer-js"
		]
	}
}
